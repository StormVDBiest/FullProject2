@model BirdMVC.Models.Result
@{
    ViewBag.Title = "Home Page";
}
<section>
    <div id="card">
        <div id="Image">
            <img src="@Model.RawImage.ImgURL" />
        </div>
        <div id="predictions">
            @foreach (BirdMVC.Models.Prediction item in Model.Predictions)
            {
                <ol>
                    <li>@item.TagName - @item.probability</li>
                </ol>
            }
        </div>
    </div>
</section>

